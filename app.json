{
  "expo": {
    "name": "CareerWise",
    "slug": "CareerWise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-nobg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/school.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F1624"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 21,
      "package": "com.nigelomondi.CareerWise"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-video",
      "expo-build-properties",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3782142596437121~3331097384",
          "iosAppId": "ca-app-pub-3782142596437121~5620851820"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "863475a0-7ffb-4556-8922-24919053e9e2"
      }
    }
  }
}
