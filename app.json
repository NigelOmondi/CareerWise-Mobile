{
  "expo": {
    "name": "CareerWise",
    "slug": "CareerWise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-nobg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/school.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F1624"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "863475a0-7ffb-4556-8922-24919053e9e2"
      }
    }
  }
}
